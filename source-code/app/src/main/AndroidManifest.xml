<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prm391.project.moneymanager">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true">
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"></activity>
        <activity
            android:name=".BudgetActivity"
            android:label="@string/title_activity_budget"></activity>
        <activity
            android:name=".ExpenseActivity"
            android:label="@string/title_activity_expense"></activity>
        <activity
            android:name=".SavingsActivity"
            android:label="@string/title_activity_savings"></activity>
        <activity
            android:name=".ReportsActivity"
            android:label="@string/title_activity_reports"></activity>
        <activity
            android:name=".ResetActivity"
            android:label="@string/title_activity_reset"></activity>
        <activity
            android:name=".SummaryActivity"
            android:label="@string/title_activity_summary"></activity>
        <activity
            android:name=".CurrencyChangeActivity"
            android:label="@string/title_currency_change"></activity>
    </application>

</manifest>